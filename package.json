{
  "name": "umi-electron-template",
  "version": "2.0.0",
  "private": true,
  "description": "umi electron template",
  "repository": "https://github.com/arvinxx/umi-electron-template.git",
  "author": "Arvin Xu<arvinx@foxmail.com>",
  "scripts": {
    "build:mac-zip": "ts-node scripts/buildMacZip.ts",
    "clean": "rm -rf dist release .eslintcache",
    "dev:main": "",
    "dev:render": "",
    "devtools": "react-devtools",
    "lint": "pnpm run lint-eslint",
    "lint-staged": "lint-staged",
    "lint:fix": "max eslint --fix",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "release": "semantic-release",
    "start": "pnpm run electron:dev",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "pnpm run lint:fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17",
    "@types/jest": "^29",
    "@types/js-yaml": "^4",
    "@types/node": "^18",
    "@types/sinon": "^10",
    "@umijs/max": "^4",
    "commitlint-config-gitmoji": "^2",
    "concurrently": "^6.5.1",
    "cross-env": "^7",
    "devtron": "^1.4.0",
    "dotenv": "^10",
    "eslint": "^8",
    "husky": "^8",
    "jest": "^29",
    "jest-sinon": "^1",
    "js-yaml": "^4",
    "lint-staged": "^10.5.3",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "react-devtools": "^4",
    "semantic-release": "^19",
    "semantic-release-config-gitmoji": "^1",
    "sinon": "^9",
    "ts-jest": "^29",
    "ts-node": "^10",
    "tslib": "^2.4.1",
    "typescript": "^4"
  }
}
