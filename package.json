{
  "name": "umi-electron-template",
  "version": "1.7.0",
  "author": "Arvin Xu<arvinx@foxmail.com>",
  "main": "main.js",
  "description": "a umi electron template",
  "private": true,
  "repository": "https://github.com/arvinxx/umi-electron-template.git",
  "scripts": {
    "start": "yarn electron:dev",
    "render:dev": "umi dev",
    "electron:dev": "PORT=7777 umi dev electron",
    "---------": "",
    "build": "umi build electron",
    "build:local": "umi build electron --dir",
    "electron:build:win": "umi build electron --win",
    "electron:build:mac": "umi build electron --mac",
    "electron:build:linux": "umi build electron --linux",
    "--------- ": "",
    "devtools": "react-devtools",
    "---------    ": "",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "---------  ": "",
    "release": "semantic-release",
    "---------   ": "",
    "lint": "yarn lint-eslint && yarn tsc",
    "tsc": "yarn tsc:renderer && yarn tsc:main",
    "tsc:renderer": "tsc -p src/renderer/tsconfig-check.json",
    "tsc:main": "tsc -p src/main/tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty src",
    "lint-staged": "lint-staged",
    "lint-styles": "stylelint",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "clean": "rm -rf dist release .eslintcache ./src/.umi",
    "---------     ": "",
    "postinstall": "umi generate tmp && electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "log4js": "^6.3.0",
    "os-name": "^4.0.0",
    "source-map-support": "^0.5.16",
    "sqlite3": "5.0.0",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-layout": "^6.14.0",
    "@commitlint/cli": "^11.0.0",
    "@jest-runner/electron": "^3.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/react": "^11.2.5",
    "@types/classnames": "^2.2.11",
    "@types/diff": "^5.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@types/sinon": "^9.0.10",
    "@umijs/fabric": "^2.5.6",
    "@umijs/preset-react": "^1.7.13",
    "@umijs/test": "^3.3.7",
    "ahooks": "^2.9.6",
    "antd": "^4.12.1",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "commitlint-config-gitmoji": "^2.2.1",
    "concurrently": "^5.3.0",
    "conventional-changelog-gitmoji-config": "^1.4.0",
    "cross-env": "^7.0.3",
    "devtron": "^1.4.0",
    "diff": "^5.0.0",
    "dotenv": "^8.2.0",
    "electron": "^11.2.2",
    "electron-builder": "^22.9.1",
    "electron-builder-notarize": "^1.2.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-is": "^3.0.0",
    "electron-log": "^4.3.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^7.19.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-sinon": "^1.0.4",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-devtools": "^4.10.1",
    "react-dom": "17.x",
    "reflect-metadata": "^0.1.13",
    "semantic-release": "^17.3.7",
    "sinon": "^9.0.0",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.2",
    "umi": "^3.3.7",
    "umi-plugin-electron-builder": "^1.0.1",
    "umi-plugin-tailwindcss": "^2.1.3"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main",
      "webpackConfig": "config/webpack.main.ts"
    },
    "renderer": null
  }
}
