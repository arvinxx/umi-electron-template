{
  "name": "umi-electron-template",
  "version": "1.2.3",
  "author": "Arvin Xu<arvinx@foxmail.com>",
  "main": "main.js",
  "description": "a umi electron template",
  "private": true,
  "repository": "https://github.com/arvinxx/umi-electron-template.git",
  "scripts": {
    "start": "yarn electron:dev",
    "render:dev": "umi dev",
    "electron:dev": "umi dev electron",
    "---------": "",
    "build": "umi build electron",
    "electron:build:win": "umi build electron --win",
    "electron:build:mac": "umi build electron --mac",
    "electron:build:linux": "umi build electron --linux",
    "--------- ": "",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "---------  ": "",
    "release": "semantic-release",
    "---------   ": "",
    "lint": "npm run lint-eslint",
    "tsc": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty src",
    "lint-staged": "lint-staged",
    "lint-styles": "stylelint",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "---------     ": "",
    "postinstall": "umi generate tmp && electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-layout": "^6.5.0",
    "@commitlint/cli": "^11.0.0",
    "@jest-runner/electron": "^3.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/react": "^11.2.3",
    "@types/classnames": "^2.2.11",
    "@types/diff": "^4.0.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@types/sinon": "^9.0.10",
    "@umijs/fabric": "^2.5.6",
    "@umijs/hooks": "^1.7.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.7",
    "antd": "^4.10.3",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "commitlint-config-gitmoji": "^2.1.2",
    "concurrently": "^5.3.0",
    "conventional-changelog-gitmoji-config": "^1.3.5",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "diff": "^4.0.1",
    "dotenv": "^8.2.0",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-builder-notarize": "^1.2.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-is": "^3.0.0",
    "electron-log": "^4.3.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "jest": "^25.1.0",
    "jest-sinon": "^1.0.4",
    "keycode": "^2.2.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "semantic-release": "^17.3.7",
    "sinon": "^9.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "umi": "^3.3.7",
    "umi-plugin-electron-builder": "^1.0.1",
    "use-merge-value": "^1.0.1"
  },
  "electronWebpack": {
    "renderer": null
  }
}
