{
  "name": "@umi-electron-template/renderer",
  "version": "2.0.0",
  "private": true,
  "description": "max electron template",
  "author": "Arvin Xu<arvinx@foxmail.com>",
  "main": "main.js",
  "scripts": {
    "build": "max build",
    "dev": "PORT=7777 max dev",
    "postinstall": "max setup",
    "lint-eslint": "max eslint",
    "lint-staged": "lint-staged",
    "lint:fix": "max eslint --fix",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "rebuild-deps": "electron-builder install-app-deps",
    "electron:init": "umi electron init",
    "electron:dev": "umi dev electron",
    "electron:build:win": "umi build electron --win",
    "electron:build:mac": "umi build electron --mac",
    "electron:build:linux": "umi build electron --linux"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "pnpm run lint:fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6",
    "@arvinxu/macos-traffic-light": "^3",
    "@umi-electron-template/common": "2.0.0",
    "@umijs/max": "^4",
    "ahooks": "^3",
    "antd": "^5",
    "antd-style": "alpha",
    "react": "18",
    "react-dom": "18",
    "react-layout-kit": "^1"
  },
  "devDependencies": {
    "@testing-library/react": "^13",
    "@types/classnames": "^2",
    "@types/fs-extra": "^9",
    "@types/jest": "^29",
    "@types/js-yaml": "^4",
    "@types/node": "^16.18.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sinon": "^10",
    "electron": "^22.0.0",
    "electron-builder": "^23",
    "fs-extra": "^11",
    "less-vars-to-js": "^1.3.0",
    "sinon": "^9",
    "umi-plugin-electron-builder": "^4"
  }
}